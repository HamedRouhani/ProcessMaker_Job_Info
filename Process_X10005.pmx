        <dyn_content><![CDATA[{"name":"درخواست ايجاد / اصلاح اطلاعات شغلي","description":"","items":[{"type":"form","variable":"","var_uid":"","dataType":"","id":"38705444362f725ae2cc315041883494","name":"درخواست ايجاد / اصلاح اطلاعات شغلي","description":"","mode":"edit","script":{"type":"js","code":"//نمايش صفحه\n\n$(\"#38705444362f725ae2cc315041883494\").css({\"backgroundColor\":\"#ccccff\"});\n$(\"#38705444362f725ae2cc315041883494\").css({\"border\":\"3px double #336699\"});\n\n\n$(\"#title0000000001\").css({\"font-family\":\"b Titr\",\"color\":\"#ffffff\",\"background-color\":\"#336699\",\"border-radius\":\"5px\"});\n$(\"#title0000000001\").css({\"text-align\":\"center\"});\n\n$(\"#submit0000000001\").css({\"font-family\":\"b titr\"});\n\n\n//پر كردن gridview  --- \n\nif ($(\"#Description\").getNumberRows() == 1 && $(\"#Description\").getValue (1,1) ==\"\") {\n\t$('#Description').getControl($(\"#Description\").getNumberRows(),'2').attr('disabled',false);\n  \t$(\"#Description\").setValue($(\"#txt_name\").getValue(),$(\"#Description\").getNumberRows(),'1');\n  }else{\n\t$(\"#Description\").addRow();\n\t$('#Description').getControl($(\"#Description\").getNumberRows(),'2').attr('disabled',false); \n    $(\"#Description\").setValue($(\"#txt_name\").getValue(),$(\"#Description\").getNumberRows(),'1');\n  };\n\n// نمايش فيلدها با توجه به نوع تغييرات\n\n$(\"#ORG_Job_Title_Search\").hide();\n$(\"#ORG_Job_Code_Selector\").hide();\n$(\"#ORG_Job_Title_Show\").hide();\n$(\"#ID_Detector\").hide();\n\n$(\"#Select_Reg_Type\").setOnchange(function(SRT,OJC) {\n  \n  SRT = $(\"#Select_Reg_Type\").getValue();\n  OJC = $(\"#ORG_Job_Code_Selector\").getValue();\n  \n  switch(true){\n   \n    case ( SRT == 1 ):\n      $(\"#ORG_Job_Title_Search\").hide();\n\t  $(\"#ORG_Job_Code_Selector\").hide();\n      $(\"#ORG_Job_Title_Show\").hide();\n      $(\"#ORG_Job_Code_Selector\").setValue(\"\");\n      $(\"#Description\").setValue(\"\",$(\"#Description\").getNumberRows(),'2');\n      $(\"#submit0000000001\").show();\n      break;\n      \n    case ( SRT == 2 ):\n      $(\"#ORG_Job_Title_Search\").show();\n\t  $(\"#ORG_Job_Code_Selector\").show();\n      $(\"#ORG_Job_Title_Show\").show();\n      $(\"#Description\").setValue(\"\",$(\"#Description\").getNumberRows(),'2');\n      $(\"#submit0000000001\").hide();\n      break;\n      \n  }\n  \n});  \n$(\"#ORG_Job_Code_Selector\").setOnchange(function(SRT,OJC,OJT) {\n  \n  SRT = $(\"#Select_Reg_Type\").getValue();\n  OJC = $(\"#ORG_Job_Code_Selector\").getValue();\n  OJT = $(\"#ORG_Job_Title_Show\").getValue();\n  \n  \n  switch(true){\n   \n    case ( OJC == \"\"):\n      $(\"#submit0000000001\").hide();\n      break;\n      \n    case ( OJC != \"\" ):\n      $(\"#submit0000000001\").show();\n      $(\"#Description\").setValue(\"اطلاعات شغلي زير جهت حذف اعلام مي گردد : \" + \"\\n\"\n                                 + OJC + \" || \" + OJT +\"\\n\"\n                                 + \"توضيحات شغل جايگزين (در صورت نياز) :\",$(\"#Description\").getNumberRows(),'2');\n      break;      \n      \n  }  \n  \n});"},"language":"en","externalLibs":"","printable":false,"items":[[{"type":"title","id":"title0000000001","label":"درخواست ايجاد / اصلاح اطلاعات شغلي","colSpan":12}],[{"type":"grid","variable":"Description","var_uid":"94302368162f726dd6cd982050055460","dataType":"grid","protectedValue":false,"id":"Description","name":"Description","label":"درخواست / نظرات","hint":"","required":false,"columns":[{"type":"text","variable":"","var_uid":"","dataType":"","protectedValue":false,"id":"txt_name1","name":"txt_name1","label":"نام و نام خانوادگي","defaultValue":"","placeholder":"","hint":"","required":false,"textTransform":"none","validate":"","validateMessage":"","maxLength":1000,"formula":"","mode":"parent","operation":"","dbConnection":"workflow","dbConnectionLabel":"PM Database","sql":"","columnWidth":"35%","width":100,"title":"نام و نام خانوادگي"},{"type":"textarea","variable":"","var_uid":"","dataType":"","prot