acement\").getValue();\n\nswitch (true) {\n \n  case ( SRT == 1):\n    $(\"#subtitle0000000001\").show();\n    $(\"#ORG_Job_Code\").show();\n    $(\"#ORG_Job_Title\").show();\n    $(\"#subtitle0000000002\").hide();\n    $(\"#ORG_Job_Code_Selector\").hide();\n    $(\"#ORG_Job_Title_Show\").hide();\n    break;\n    \n  case ( SRT == 2 && REP == 1):\n    $(\"#subtitle0000000001\").show();\n    $(\"#ORG_Job_Code\").show();\n    $(\"#ORG_Job_Title\").show();\n    $(\"#subtitle0000000002\").show();\n    $(\"#ORG_Job_Code_Selector\").show();\n    $(\"#ORG_Job_Title_Show\").show();\n    break;\n    \n  case ( SRT == 2 && REP == 0):\n    $(\"#subtitle0000000001\").hide();\n    $(\"#ORG_Job_Code\").hide();\n    $(\"#ORG_Job_Title\").hide();\n    $(\"#subtitle0000000002\").show();\n    $(\"#ORG_Job_Code_Selector\").show();\n    $(\"#ORG_Job_Title_Show\").show();\n    break;     \n    \n}"},"language":"en","externalLibs":"","printable":false,"items":[[{"type":"title","id":"title0000000001","label":"ثبت و اطلاع‌رساني اطلاعات","colSpan":12}],[{"type":"grid","variable":"Description","var_uid":"94302368162f726dd6cd982050055460","dataType":"grid","protectedValue":false,"id":"Description","name":"Description","label":"درخواست / نظرات","hint":"","required":false,"columns":[{"type":"text","variable":"","var_uid":"","dataType":"","protectedValue":false,"id":"txt_name1","name":"txt_name1","label":"نام و نام خانوادگي","defaultValue":"","placeholder":"","hint":"","required":false,"textTransform":"none","validate":"","validateMessage":"","maxLength":1000,"formula":"","mode":"parent","operation":"","dbConnection":"workflow","dbConnectionLabel":"PM Database","sql":"","columnWidth":"30%","width":100,"title":"نام و نام خانوادگي"},{"type":"textarea","variable":"","var_uid":"","dataType":"","protectedValue":false,"id":"Desc_Details","name":"Desc_Details","label":"شرح","defaultValue":"","placeholder":"","hint":"","required":false,"validate":"","validateMessage":"","mode":"parent","dbConnection":"w